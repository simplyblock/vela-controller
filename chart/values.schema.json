{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["gateway", "issuer"],
  "properties": {
    "gateway": {
      "type": "object",
      "required": ["name", "namespace"],
      "properties": {
        "name": {
          "type": "string"
        },
        "namespace": {
          "type": "string"
        }
      }
    },
    "issuer": {
      "type": "string"
    }
  }
}
